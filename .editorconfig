# http://editorconfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
# 缩进风格
# 可选的值为-   tab, space
indent_style = space
# 缩进几个tab ,空格
indent_size = 2
end_of_line = lf
# 设置编码
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
[{package.json,.travis.yml}]
indent_style = space
indent_size = 4
